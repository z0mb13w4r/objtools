static convert_t ecSHNINDEX[] = {
  {"UND",  SHN_UNDEF},
  {"ABS",  SHN_ABS},
  {"COM",  SHN_COMMON},
  {0, 0}
};

static const char* get_SHNINDEX(const pick_t x) {
  static char buff[32];

  const char* def = strpicknull(ecSHNINDEX, x);
  if (NULL == def) {
    snprintf(buff, sizeof(buff), "%3ld", x);
  } else {
    snprintf(buff, sizeof(buff), "%3s", def);
  }

  return buff;
}

