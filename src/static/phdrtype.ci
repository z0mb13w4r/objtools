};

#define PT_RISCV_ATTRIBUTES            (0x70000003)

convert_t ecPHDRTYPERISCV[] = {
  { "RISCV_ATTRIBUTES",            PT_RISCV_ATTRIBUTES},

  {0, 0}
};

pconvert_t get_PHDRTYPE(const pbuffer_t p, const uint64_t p_type) {
  if ((p_type >= PT_LOOS) && (p_type <= PT_HIOS)) {
    return ecPHDRTYPEOS;
  } else if ((p_type >= PT_LOPROC) && (p_type <= PT_HIPROC)) {
    switch (ecget_emachine(p)) {
    case EM_MIPS:
    case EM_MIPS_RS3_LE:
      return ecPHDRTYPEMIPS;

    case EM_ARM:
      return ecPHDRTYPEARM32;

    case EM_AARCH64:
      return ecPHDRTYPEARM64;

    case EM_RISCV:
      return ecPHDRTYPERISCV;

    default:
      break;
    }

  }

  return ecPHDRTYPE;
}

