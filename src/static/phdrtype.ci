};

#define PT_AARCH64_ARCHEXT             (PT_LOPROC + 0)
#define PT_AARCH64_MEMTAG_MTE          (PT_LOPROC + 2)

convert_t ecPHDRTYPEARM64[] = {
  {"AARCH64_ARCHEXT",              PT_AARCH64_ARCHEXT},
  {"AARCH64_MEMTAG_MTE",           PT_AARCH64_MEMTAG_MTE},

