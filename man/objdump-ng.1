.IX Item "-w[aAcFikKLmrRsptTuUgX]"
.PD 0
.IP "\fB\-\-debug-dump[=abbrev,=addr,=cu_index,=frames-interp,=info,=links,=follow-links,=decodedline,=macro,=aranges,=ranges,=str,=pubnames,=pubtypes,=trace_aranges,=trace_abbrev,=trace_info,=gdb_index,=enhanced]\fR" 4
